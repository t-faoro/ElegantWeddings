	float: left;
	height: 100%;
	width: 260px;
	z-index: 3;
}

div#footerWrap{
	opacity: 0.75;
	position: relative;
	border-radius: 10px;
	margin: 0 auto;
	margin-top: 25px;
	width: 960px;
	padding-left: 25px;
	padding-right: 25px;
	background-color: #F3F6EF;
