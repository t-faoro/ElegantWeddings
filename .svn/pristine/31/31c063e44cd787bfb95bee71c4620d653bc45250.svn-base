div#nav ul li.selected a{
	background: -webkit-gradient(linear, left top, left bottom, from(#666), to(#333));
	background: -moz-linear-gradient(top,  #666,  #333);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#666', endColorstr='#333');
	background: -ms-linear-gradient(top, #666, #333);
}

div#nav a.first{
	border-top-left-radius: 25px;
	border-bottom-left-radius: 25px;
}

div#nav a.last{
	border-top-right-radius: 25px;
	border-bottom-right-radius: 25px;
}

div#nav a:hover{

	background: -webkit-gradient(linear, left top, left bottom, from(#5E307A), to(#999));
	background: -moz-linear-gradient(top,  #5E307A,  #999);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#5E307A', endColorstr='#999');
	background: -ms-linear-gradient(top, #5E307A, #999);
	
}

