/*Main Heading Ribbon*/
.ribbon {
  font-size: 22px !important;
  /* This ribbon is based on a 16px font side and a 24px vertical rhythm. I've used em's to position each element for scalability. If you want to use a different font size you may have to play with the position of the ribbon elements */
  left: 20%;
  width: 50%;
  position: relative;
  background: #643A7D;
  color: #fff;
  text-align: center;
  padding: 1em 2em; /* Adjust to suit */
  margin: 0 0 3em; /* Based on 24px vertical rhythm. 48px bottom margin - normally 24 but the ribbon 'graphics' take up 24px themselves so we double it. */
-moz-box-shadow: 0px 5px 5px #666;
-webkit-box-shadow: 0px 5px 5px #666;
box-shadow: 0px 5px 5px #666;
}
.ribbon:before, .ribbon:after {
  content: "";
  position: absolute;
  display: block;
  bottom: -1em;
  border: 1.5em solid #765B87;
  z-index: -1;
}
.ribbon:before {
  left: -2em;
  border-right-width: 1.5em;
  border-left-color: transparent;
}
.ribbon:after {
  right: -2em;
  border-left-width: 1.5em;
  border-right-color: transparent;
}
.ribbon .ribbon-content:before, .ribbon .ribbon-content:after {
  content: "";
  position: absolute;
  display: block;
  border-style: solid;
  border-color: #333 transparent transparent transparent;
  bottom: -1em;
  
}
.ribbon .ribbon-content:before {
  left: 0;
  border-width: 1em 0 0 1em;
}
.ribbon .ribbon-content:after {
  right: 0;
  border-width: 1em 1em 0 0;
}

/*SubHeading Ribbon*/
.subRibbon {
  font-size: 18px !important;
  /* This ribbon is based on a 16px font side and a 24px vertical rhythm. I've used em's to position each element for scalability. If you want to use a different font size you may have to play with the position of the ribbon elements */
  left: 32%;
  width: 25%;
  position: relative;
  background: #333;
  color: #fff;
  text-align: center;
  padding: 1em 2em; /* Adjust to suit */
  margin: 0 0 3em; /* Based on 24px vertical rhythm. 48px bottom margin - normally 24 but the ribbon 'graphics' take up 24px themselves so we double it. */
-moz-box-shadow: 0px 5px 5px #666;
-webkit-box-shadow: 0px 5px 5px #666;
box-shadow: 0px 5px 5px #666;
}
.subRibbon:before, .subRibbon:after {
  content: "";
  position: absolute;
  display: block;
  bottom: -1em;
  border: 1.5em solid #666;
  z-index: -1;
}
.subRibbon:before {
  left: -2em;
  border-right-width: 1.5em;
  border-left-color: transparent;
}
.subRibbon:after {
  right: -2em;
  border-left-width: 1.5em;
  border-right-color: transparent;
}
.subRibbon .subRibbon-content:before, .subRibbon .subRibbon-content:after {
  content: "";
  position: absolute;
  display: block;
  border-style: solid;
  border-color: #333 transparent transparent transparent;
  bottom: -1em;
  
}
.subRibbon .subRibbon-content:before {
  left: 0;
  border-width: 1em 0 0 1em;
}
.subRibbon .subRibbon-content:after {
  right: 0;
  border-width: 1em 1em 0 0;
}

/*Top Ribbon*/
.topRibbon {
  font-size: 16px !important;
  /* This ribbon is based on a 16px font side and a 24px vertical rhythm. I've used em's to position each element for scalability. If you want to use a different font size you may have to play with the position of the ribbon elements */
  left: 12%;
  border-radius: 5px;
  bottom: 5px;;
  width: 85%;
  position: relative;
  background: #CBCBCB;
  color: #333;
  text-align: center;
  padding: 1em 2em; /* Adjust to suit */
  margin: 0 0 3em; /* Based on 24px vertical rhythm. 48px bottom margin - normally 24 but the ribbon 'graphics' take up 24px themselves so we double it. */
-moz-box-shadow: 0px 5px 5px #666;
-webkit-box-shadow: 0px 5px 5px #666;
box-shadow: 0px 5px 5px #666;
}
 .topRibbon:after {
  content: "";
  position: absolute;
  display: block;
  bottom: -1em;
  border: 1.5em solid #666;
  z-index: -10;
}
/*.topRibbon:before {
  left: -2em;
  border-right-width: 1.5em;
  border-left-color: transparent;
}*/
.topRibbon:after {
  right: -2em;
  border-left-width: 1.5em;
  border-right-color: transparent;

}
.topRibbon .topRibbon-content:after {
  content: "";
  position: absolute;
  z-index: -1;
  display: block;
  border-style: solid;
  border-color: #333 transparent transparent transparent;
  bottom: -1em;
  
}
.topRibbon .topRibbon-content:before {
  left: 0;
  border-width: 1em 0 0 1em;
}
.topRibbon .topRibbon-content:after {
  right: 0;
  border-width: 1em 1em 0 0;
}

div#contactForm{
	border: 1px solid #666;
	box-shadow: 0px 5px 10px #666;
	padding: 15px;
	width: 65%;
	background: #CBCBCB;
}

div#contactForm form label{
	display: inline-block;
	width: 100px;
	font-weight: bold;
	font-size: 0.85em;
}

div#contactForm form label + input[type="text"]{
	width: 275px;
	height: 20px;
	margin-bottom: 5px;
	font-size: 0.85em;
	padding: 5px 10px 5px 10px;
	box-shadow: inset 1px 0px 9px #666;
	border: 0;
	border-radius: 5px;
}

div#contactForm form label#commentLabel + textarea{
	width: 275px;
	height: 150px;
	margin-bottom: 5px;
	font-size: 0.85em;
	padding: 5px 10px 5px 10px;
	box-shadow: inset 1px 0px 9px #666;
	border: 0;
	border-radius: 5px;
}

span.commentContainer *{
	vertical-align:top;
}

span.note{
	position: relative;
	top: -10px;
	font-size: 0.75em;
	
}

input[type="submit"]{
    background-color: #5D9AD0;
    background-image: linear-gradient(to bottom, #5E307A, #999);
    background-repeat: repeat-x;
	border: 1px solid #CBCBCB;
    box-shadow: 0 1px 0 rgba(255, 255, 255, 0.2) inset, 0 1px 2px rgba(0, 0, 0, 0.05);
    color: #FFF;
    cursor: pointer;
    display: inline-block;
    font-size: 14px;
	font-weight: bold;
    line-height: 20px;
    margin-bottom: 0;
	margin-top: 10px;
    padding: 5px 12px;
    text-align: center;
    text-shadow: 1px 1px rgba( 0, 0, 0, .8);
    vertical-align: middle;
	min-width: 135px;
	border-radius: 5px;
	min-height: 30px;
}



