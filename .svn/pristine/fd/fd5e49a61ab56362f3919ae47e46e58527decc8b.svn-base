}


div#nav a{
	display: block;
	padding:15px;
	padding-left: 25px;
	padding-right: 25px;
	width: 100px;
	font-size: 20px;
	border-left: solid;
	border-right: solid;
	border-color: #806380;
	border-width: 1px;
	text-decoration: none;
	text-align: center;
	color: #FFF;
	background: -webkit-gradient(linear, left top, left bottom, from(#5E307A), to(#333));
	background: -moz-linear-gradient(top,  #5E307A,  #333);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#5E307A', endColorstr='#333');
		
}

div#nav a:hover{

	background: -webkit-gradient(linear, left top, left bottom, from(#5E307A), to(#999));
	background: -moz-linear-gradient(top,  #5E307A,  #999);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#5E307A', endColorstr='#999');
	
}

div#footerContent{
	width: 960px;
	height: 100%;
	margin: 0 auto;	
}

div.leftNoMargin{
	margin: 0;
	padding: 15px;
	float: left;
	border-right: solid;
	border-color: #000;
	border-width: 1px;
	
}

div.left{
	border-right: solid;
	border-color: #000;
	border-width: 1px;
	margin-left: 25px;
	float: left;
}


