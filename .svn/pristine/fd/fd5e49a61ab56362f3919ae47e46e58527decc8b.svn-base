html{
	font-size: 100%;
	font-family: Arial, Helvetica, sans-serif;
	overflow-x: hidden;
}

body{
	background-image: url('../images/background.jpg');
	background-position: top center;
	background-repeat: repeat;
	color: #333;
}

h1,h2,h3,h4,h5{
	margin: 0 40px 0 40px;
	margin-top: 25px;
	margin-bottom: 25px;
	font-family: Georgia, serif;
	color: #5E005E;
}

p{
	margin: 0 40px 0 40px;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 16px;
	line-height: 22px;
	color: #403152;
	width: auto;
}

a{
	text-decoration: none;	
}

a:hover{
	color: white;
	text-decoration: underline;
}



/* Containers */

div#container{
	width: 100%;
	height: 100%;
}

div#headerWrap{
